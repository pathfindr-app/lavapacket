{
  "version": 2,
  "rewrites": [
    {
      "source": "/packets/new",
      "destination": "/packets/builder.html"
    },
    {
      "source": "/packets/:id",
      "destination": "/packets/builder.html?id=:id"
    },
    {
      "source": "/inspections/new",
      "destination": "/inspections/form.html"
    },
    {
      "source": "/inspections/:id",
      "destination": "/inspections/form.html?id=:id"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
