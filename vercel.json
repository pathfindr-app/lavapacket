{
  "version": 2,
  "routes": [
    {
      "src": "/packets/new",
      "dest": "/packets/builder.html"
    },
    {
      "src": "/packets/([^/]+)",
      "dest": "/packets/builder.html?id=$1"
    },
    {
      "src": "/inspections/new",
      "dest": "/inspections/form.html"
    },
    {
      "src": "/inspections/([^/]+)",
      "dest": "/inspections/form.html?id=$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
